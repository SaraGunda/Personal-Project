<%- include('../partials/head') -%> <%- include('../partials/navbar') -%>

<div class="loginContainer">
  <form>
    <h1>Login</h1>
    <div class="form-group">
      <label for="email"
        >Email Address <span class="form-group-required">*</span></label
      >
      <input
        type="email"
        name="email"
        id="name"
        placeholder="Your email address"
        required
      />
    </div>
    <div class="form-group">
      <label for="password"
        >Password <span class="form-group-required">*</span></label
      >
      <input
        type="password"
        name="password"
        id="name"
        placeholder="*******"
        required
      />
    </div>
    <button type="submit">Submit</button>
  </form>
</div>
<%- include('../partials/footer') -%>
